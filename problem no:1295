class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int n=nums.size();
        int cnt=0;
        
        for(int i=0;i<n;i++){
            int num=nums[i];
             int p=0;
            while(num){
               // int k=num;
               
               num=num/10;
                p++;
                

            }
            if(p%2==0){
                cnt++;
            }
        }
        return cnt;
        
    }
};
